@{
    ViewData["Title"] = "VerDetalleCandidato";
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="style.css">
    <title>Detalles Candidatos</title>
</head>
<body>
    <h1>Informaci√≥n sobre @ViewBag.VerDetalleCandidato.Nombre  @ViewBag.VerDetalleCandidato.Apellido</h1>
<img src="/Fotos/@ViewBag.VerInfoCandidato.Foto">
<p>Fecha de nacimiento: @ViewBag.VerDetalleCandidato.FechaNacimiento.ToShortDateString()</p>
<p>ID: @ViewBag.VerDetalleCandidato.IdCandidato</p>
<p>ID del partido: @ViewBag.VerDetalleCandidato.IdPartido</p>
<p>Postulacion: @ViewBag.VerDetalleCandidato.Postulacion</p>

<a href='@Url.Action("VerDetallePartido",new {IdPartido=@ViewBag.VerDetalleCandidato.idPartido}),@ViewBag.VerDetalleCandidato.IdPartido'><button>Volver al partido</button></a>

